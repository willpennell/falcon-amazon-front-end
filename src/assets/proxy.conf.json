{
  "/api/user/create": {
    "target": "http://localhost:8080",
    "secure": true,
    "changeOrigin": true,
    "pathRewrite": {
      "^/api/user/create": "/api/user/create"
    }
  },
  "/api/user/*": {
    "target": "http://localhost:8080",
    "secure": true,
    "changeOrigin": true,
    "pathRewrite": {
      "^/api/user/": "/api/user/"
    }
  }
}

  
